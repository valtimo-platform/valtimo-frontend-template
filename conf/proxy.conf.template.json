    "changeOrigin": true
  },
  "/management/*": {
    "target": "${BACKEND_URL}",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/v3/*": {
    "target": "${BACKEND_URL}",
    "secure": false,
